    "@vitejs/plugin-react": "^4.3.3",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.2.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "typescript": "~5.6.2",
    "vite": "^5.4.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
